{
    "name": "aaemnnosttv/boiler",
    "description": "_____________",
    "type": "wordpress-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Evan Mattson",
            "email": "me@aaemnnost.tv"
        }
    ],
    "config": {
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Boiler\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "BoilerTests\\": "tests"
        }
    },
    "require": {
        "composer/installers": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.3",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "scripts": {
        "lint": [
            "vendor/bin/phpcs"
        ]
    }
}
